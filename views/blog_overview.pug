                            .media.post_author
                                .round_img
                                    img(src='img/blog-grid/author_2.jpg' alt='')
                                .media-body.author_text
                                    h4 Druid Wensleydale
                                    .date Sep 14, 2020
                .col-lg-4.col-sm-6
                    .blog_grid_post.wow.fadeInUp(data-wow-delay='0.4s')
                        img(src='https://media.istockphoto.com/photos/teenager-girl-doing-her-homework-stock-photo-picture-id1214280041?k=20&m=1214280041&s=612x612&w=0&h=IUC1n6voqi0zWdEL2hvbpUcfRpvZakCFp8HxEqcIiDM=' alt='')
                        .grid_post_content
                            .post_tag
                                a(href='#') 10 Min Read
                                a.cat-laravel.green(href='#') Laravel
                            a(href='blog-single.html')
