                            .media.post_author
                                .round_img
                                    img(src='img/blog-grid/author_3.jpg' alt='')
                                .media-body.author_text
                                    h4 Douglas Lyphe
                                    .date Nov 10, 2020
    section.doc_action_area_three
        img.p_absolute.shadows(src='img/blog-grid/shadow_action.png' alt='')
        img.p_absolute.b_man(src='img/blog-grid/man.png' alt='')
        .container
            .row
                .col-lg-5
                    .action_content
                        h2
                            | Want to read more about your favourite 
                            span topic?
                        p Get regular updates in your inbox
                .col-lg-7
                    form.footer_subscribe_form.action_subscribe_form(action='#')
                        .form-group
                            input.form-control(type='email' placeholder='Email Address')
                            button.s_btn(type='submit')
                                | Subcribe Now 
                                i.arrow_right
                        .form-check
                            input#defaultCheck1.form-check-input(type='checkbox' value='')
                            label.form-check-label(for='defaultCheck1')
                                | I agree to the KbDoc
    include _footer
// Back to top button
a#back-to-top(title='Back to Top')
include _js
