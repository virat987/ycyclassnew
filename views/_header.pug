style.
  @import url('https://fonts.googleapis.com/css2?family=Roboto:wght@500&display=swap');
  * {
  margin: 0;
  padding: 0;
  border: none;
  outline: none;
  box-sizing: border-box;
  }
  header {
  display: flex;
  height: 85px;
  align-items: center;
  z-index:5;
  position:relative;
  }
  .usergc {
  display: none;
  }
  #iconparent {
  width: 120px;
  flex-grow: initial;
  text-align: center;
  }
  .acimage {
  width: 70px;
  display: block;
  margin: auto;
  }
  #textet {
  font-size: 16px;
  padding: 7px 15px 7px 15px;
  background-color: #f0f0f0;
  border-radius: 20px;
  position: static;
  width: 290px;
  flex-grow: 1;
  max-width: 350px;
  height: auto;
  margin-left: 20px;
  }
  #spacemana {
  flex-grow: 1;
  }
  #mobilelinks {
  display: flex;
  justify-content: space-around;
  max-width: 660px;
  align-items: center;
  flex-grow: 1;
  padding-right: 20px;
  }
  #mobilelinks p,
  #mobilelinks div {
  font-family: 'Roboto', sans-serif;
  padding-left: 8px;
  font-size: 17px;
  margin-bottom: 0;
  font-weight: 410;
  }
  #mobilelinks p:hover,
  .mnjh:hover,
  #mobilelinks span:hover {
  opacity: 0.6;
  cursor: pointer;
  }
  .seagc,
  .usergcc {
  padding-right: 20px;
  margin-bottom: 0;
  }
  .seagc {
  display: none;
  }
  .oveflowheadhaj {
  position: relative;
  height: 20px;
  line-height:initial;
  color:#262626;
  }
  .overflowheadetr {
  position: relative;
  top: 40px;
  padding: 4px;
  border-radius: 10px;
  width: 100%;
  left: 0px;
  background-color: #f0f0f0;
  box-shadow: 2px 2px 2px rgba(0, 0, 0, 0.24);
  opacity: 0;
  transition: opacity 300ms, top 300ms;
  }
  .overflowheadetr::after {
  content: " ";
  position: absolute;
  bottom: 100%;
  left: 50%;
  margin-left: -8px;
  border-width: 8px;
  border-style: solid;
  border-color: transparent transparent #f0f0f0 transparent;
  }
  .overflowheadetr span {
  display: block;
  padding-top: 10px;
  padding-bottom: 10px;
  text-align: center;
  }
  .overflowheadetr span:hover {
  opacity: 0.6;
  }
  .loremespusmms {
  display: none;
  }

  #notes {
    width:50px;
  }
  @media (max-width:1000px) {
  header {
  /* background: url("./Component.png"); */
  display: flex;
  align-items: center;
  height: 60px;
  background-size: 100% auto;
  position: relative;
  padding-left: 10px;
  padding-right: 10px;
  }
  .loremespusmms {
  display: block;
  position: absolute;
  top: 12px;
  right: 0px;
  width: 14px;
  height: 14px;
  }
  .usergc {
  display: block;
  margin-bottom: 0;
  }
  .notesss {
  display: block;
  }
  #iconparent {
  flex-grow: 1;
  }
  #spacemana {
  display: none;
  }
  .acimage {
  width: 70px;
  display: block;
  margin: auto;
  }
  .seagc {
  display: block;
  }
  #textet {
  width: 35px;
  height: 33px;
  font-size: 17px;
  padding: 5px 15px 5px 15px;
  background-color: #f0f0f0;
  max-width: initial;
  position: absolute;
  right: 38px;
  margin-left: 0px;
  top: 14px;
  border-radius: 20px;
  animation-duration: 400ms;
  opacity: 0;
  cursor: pointer;
  animation-fill-mode: forwards;
  animation-iteration-count: 1;
  animation-direction: normal;
  transition: width 400ms, opacity 400ms;
  }
  #textet:hover {
  opacity: 1;
  width: calc(100% - 80px);
  }
  .usergc:hover~#mobilelinks {
  left: 0;
  }
  #mobilelinks:hover {
  left: 0;
  }
  #mobilelinks {
  display: block;
  position: absolute;
  top: 0px;
  margin-top: 60px;
  left: 0;
  height: calc(100vh - 60px);
  padding: 20px;
  background-color: #262626;
  left: -200px;
  max-width: initial;
  transition: left 400ms;
  z-index: 6;
  color: white;
  }
  #mobilelinks p {
  font-family: 'Roboto', sans-serif;
  font-size: 14px;
  padding-top: 12px;
  padding-bottom: 12px;
  padding-left: auto;
  color: white;
  }
  #mobilelinks p,
  #mobilelinks>div {
  font-size: 14px;
  padding-top: 12px;
  padding-bottom: 12px;
  height: 40px;
  width: 150px;
  overflow: hidden;
  font-weight: 410;
  }
  #mobilelinks span {
  font-size: 14px;
  font-weight: 410;
  }
  .seagc,
  .usergcc {
  padding-right: 0px;
  }
  .oveflowheadhaj {
  position: relative;
  top: 0;
  left: 0;
  height: auto;
  transition: height 300ms;
  color:white;
  }
  .overflowheadetr {
  position: static;
  top: initial;
  padding: initial;
  background-color: transparent;
  box-shadow: none;
  opacity: 1;
  padding-left: 0;
  text-align: left;
  }
  .overflowheadetr::after {
  content: "";
  }
  .overflowheadetr span {
  display: block;
  padding-top: 12px;
  padding-bottom: 11px;
  text-align: left;
  }
  .overflowheadetr span:hover {
  opacity: 0.6;
  }
  }
script.
  let downsvg = `<svg  aria-hidden="true" focusable="false" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 512 512" data-fa-i2svg=""><path fill="currentColor" d="M233.4 406.6c12.5 12.5 32.8 12.5 45.3 0l192-192c12.5-12.5 12.5-32.8 0-45.3s-32.8-12.5-45.3 0L256 338.7 86.6 169.4c-12.5-12.5-32.8-12.5-45.3 0s-12.5 32.8 0 45.3l192 192z"></path></svg>`;
  let upsvg = `<svg  aria-hidden="true" focusable="false"  xmlns="http://www.w3.org/2000/svg" viewBox="0 0 512 512" data-fa-i2svg=""><path fill="currentColor" d="M233.4 105.4c12.5-12.5 32.8-12.5 45.3 0l192 192c12.5 12.5 12.5 32.8 0 45.3s-32.8 12.5-45.3 0L256 173.3 86.6 342.6c-12.5 12.5-32.8 12.5-45.3 0s-12.5-32.8 0-45.3l192-192z"></path></svg>`;
  let doubtsolve = true;
  document.getElementById("instantdddd").addEventListener("mouseenter", (event) => {
  if (window.innerWidth > 1000) {
  document.getElementById("overflowconrrrr1").style.opacity = "1";
  document.getElementById("overflowconrrrr1").style.top = "20px";
  doubtsolve = true;
  }
  });
  document.getElementById("instantdddd").addEventListener("mouseleave", () => {
  if (window.innerWidth > 1000) {
  setTimeout(() => {
  if (doubtsolve) {
  document.getElementById("overflowconrrrr1").style.opacity = "0";
  document.getElementById("overflowconrrrr1").style.top = "40px";
  }
  }, 100);
  }
  });
  document.getElementById("overflowconrrrr1").addEventListener("mouseenter", () => {
  if (window.innerWidth > 1000) {
  doubtsolve = false;
  }
  })
  document.getElementById("overflowconrrrr1").addEventListener("mouseleave", () => {
  if (window.innerWidth > 1000) {
  document.getElementById("overflowconrrrr1").style.opacity = "0";
  document.getElementById("overflowconrrrr1").style.top = "40px";
  doubtsolve = true;
  }
  });
  let notesolve = true;
  document.getElementById("instantdddd2").addEventListener("mouseenter", (event) => {
  if (window.innerWidth > 1000) {
  document.getElementById("overflowconrrrr2").style.opacity = "1";
  document.getElementById("overflowconrrrr2").style.top = "20px";
  notesolve = true;
  }
  });
  document.getElementById("instantdddd2").addEventListener("mouseleave", () => {
  if (window.innerWidth > 1000) {
  setTimeout(() => {
  if (notesolve) {
  document.getElementById("overflowconrrrr2").style.opacity = "0";
  document.getElementById("overflowconrrrr2").style.top = "40px";
  }
  }, 100);
  }
  });
  document.getElementById("overflowconrrrr2").addEventListener("mouseenter", () => {
  if (window.innerWidth > 1000) {
  notesolve = false;
  }
  })
  document.getElementById("overflowconrrrr2").addEventListener("mouseleave", () => {
  if (window.innerWidth > 1000) {
  document.getElementById("overflowconrrrr2").style.opacity = "0";
  document.getElementById("overflowconrrrr2").style.top = "40px";
  notesolve = true;
  }
  });
  document.getElementsByClassName("usergc")[0].addEventListener("click", (event) => {
  let value = document.getElementsByClassName("usergc")[0].getAttribute("on")
  if (value === "on") {
  document.getElementById("mobilelinks").style.left = "-200px";
  document.getElementsByClassName("usergc")[0].setAttribute("on", "off");
  } else {
  document.getElementById("mobilelinks").style.left = "0px";
  document.getElementsByClassName("usergc")[0].setAttribute("on", "on");
  }
  });
  [...document.getElementsByClassName("loremespusmms")].forEach(elem => {
  elem.addEventListener("click", (event) => {
  let value = elem.getAttribute("open");
  if (value == "opened") {
  elem.innerHTML = downsvg;
  elem.parentElement.style.height = "40px";
  elem.setAttribute("open", "closed");
  } else {
  elem.innerHTML = upsvg;
  elem.parentElement.style.height = "110px";
  elem.setAttribute("open", "opened");
  }
  })
  });
  window.onclick = function (event) {
  let value = document.getElementsByClassName("usergc")[0].getAttribute("on")
  if (value == "on") {
  if(event.pageX > 190){
  document.getElementById("mobilelinks").style.left = "-200px";
  document.getElementsByClassName("usergc")[0].setAttribute("on", "off");
  }
  }
  }
  document.getElementById("textet").addEventListener("keydown", (event) => {
  if (event.keyCode == 13 && event.target.value != "") {
  window.location="/search-question-question/"+decodeURI(event.target.value);
  }
  })
